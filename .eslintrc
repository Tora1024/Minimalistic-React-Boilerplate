{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins":[
        "react"
    ],
    "globals": {
        "module": 0
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"]
}